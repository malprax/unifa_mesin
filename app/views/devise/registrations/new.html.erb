<div class="row-fluid">
  <div class="span12">
    <div class="span4"></div>
    <div class="span4">
      <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>

        <div class="field">
          <hr>
          <%= f.text_field :username, placeholder: "username" %></div></br>

        <div>
        <%= f.email_field :email, :autofocus => true, placeholder: "email" %></div> </br>      
          

        <div>
        <%= f.password_field :password, placeholder: "password" %></div></br>

        <div>
        <%= f.password_field :password_confirmation, placeholder: "ulangi password" %></div></br>
  
          <div><%= f.submit "Daftar" %></div>
      <% end %>

      <%= render "devise/shared/links" %>
    </div>
    <div class="span4"></div>
  </div>
</div>
<hr>



