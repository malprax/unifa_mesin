<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">No</Data></Cell>
        <Cell><Data ss:Type="String">Kode</Data></Cell>
        <Cell><Data ss:Type="String">Mata Kuliah</Data></Cell>
        <Cell><Data ss:Type="String">Teori(sks)</Data></Cell>
        <Cell><Data ss:Type="String">Lab(sks)</Data></Cell>
        <Cell><Data ss:Type="String">Jumlah(sks)</Data></Cell>
        <Cell><Data ss:Type="String">Semester</Data></Cell>
        <Cell><Data ss:Type="String">Mata Kuliah Pilihan</Data></Cell>
      </Row>
    <% @matakuliah.each_with_index do |m,i| %>
      <Row>
        <Cell><Data ss:Type="Number"><%= i + 1%></Data></Cell>
        <Cell><Data ss:Type="String"><%= m.kode%></Data></Cell>
        <Cell><Data ss:Type="String"><%= m.mata_kuliah%></Data></Cell>
        <Cell><Data ss:Type="Number"><%= m.teori%></Data></Cell>
        <Cell><Data ss:Type="Number"><%= m.lab %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= m.teori + m.lab %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= m.semester %></Data></Cell>
        <Cell><Data ss:Type="Boolean"><%= m.mata_kuliah_pilihan == false ? "ya" : "tidak" %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>